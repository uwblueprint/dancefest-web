    "dayjs": "^1.10.4",
    "formik": "^2.2.6",
    "husky": "4",
    "json2csv": "^5.0.6",
    "next": "11.1.1",
    "next-auth": "^3.4.2",
    "next-images": "^1.7.0",
    "next-s3-upload": "^0.1.4",
    "nodemailer": "^6.4.18",
    "pg": "^8.5.1",
    "react": "17.0.2",
    "react-csv": "^2.0.3",
    "react-datepicker": "^3.7.0",
    "react-dom": "17.0.2",
