    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prisma": "^2.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,scss,md}": "prettier --write"
