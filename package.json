  "dependencies": {
    "@prisma/client": "2.17.0",
    "aws-sdk": "^2.848.0",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "formik": "^2.2.6",
    "next": "10.0.7",
    "next-auth": "^3.4.2",
    "next-images": "^1.7.0",
    "nodemailer": "^6.4.18",
    "pg": "^8.5.1",
    "react": "17.0.1",
    "react-datepicker": "^3.5.0",
    "react-dom": "17.0.1",
    "react-loader-spinner": "^4.0.0",
    "react-modal": "^3.12.1",
    "react-paginate": "^7.1.0",
    "react-select": "^4.1.0",
    "react-table": "^7.6.3",
    "sass": "^1.32.7",
    "yocto-queue": "^0.1.0"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "=4",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prisma": "2.17.0",
    "serverless": "1.74.1"
  },
  "husky": {
    "hooks": {
